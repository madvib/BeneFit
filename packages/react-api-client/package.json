{
  "name": "@bene/react-api-client",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./fixtures": {
      "types": "./src/generated/fixtures/index.ts",
      "default": "./src/generated/fixtures/index.ts"
    }
  },
  "peerDependencies": {
    "@bene/shared": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "agents": "catalog:",
    "better-auth": "catalog:",
    "hono": "4.11.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"    
  },
  "nx": {
    "implicitDependencies": [
      "@bene/gateway"
    ],
    "targets": {
      "build": {
        "dependsOn": [
          "@bene/training-core:build",
          "@bene/training-application:build",
          "@bene/coach-domain:build",
          "@bene/integrations-domain:build",
          "@bene/gateway:build-client",
          "default"
        ]
      }
    },
    "tags": [
      "scope:client",
      "type:library",
      "layer:interface"
    ]
  }
}
