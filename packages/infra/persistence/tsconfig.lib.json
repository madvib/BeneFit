{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "resolveJsonModule": false,
    "moduleResolution": "bundler",
    "module": "esnext",
    "types": [
      "node",
    ]
  },
  "include": [
    "../../../types/cloudflare-runtime.d.ts",
    "src/**/*.ts",
    "src/**/migrations/**/*.json",
    "src/**/migrations/**/*.js"
  ],
  "references": [],
  "exclude": [
    "vite.config.ts",
    "vite.config.mts",
    "vitest.config.ts",
    "vitest.config.mts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.test.jsx",
    "src/**/*.spec.jsx"
  ]
}