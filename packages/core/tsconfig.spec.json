{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/vitest",
    "types": [
      "vitest/globals",
      "vitest/importMeta",
      "vite/client",
      "node",
      "vitest"
    ],
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@shared": [
        "./src/shared/index.ts"
      ],
      "@profile": [
        "./src/profile/index.ts"
      ],
      "@plans": [
        "./src/plans/index.ts"
      ],
      "@integrations": [
        "./src/integrations/index.ts"
      ],
      "@auth": [
        "./src/auth/index.ts"
      ],
      "@coach": [
        "./src/coach/index.ts"
      ],
      "@blog": [
        "./src/blog/index.ts"
      ],
      "@workouts": [
        "./src/workouts/index.ts"
      ]
    }
  },
  "include": [
    "vitest.config.ts",
    "vitest.config.mts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.test.jsx",
    "src/**/*.spec.jsx",
    "src/**/*.d.ts",
    "src/**/*.ts"
  ],
}