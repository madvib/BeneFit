{
  "name": "@bene/web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bene/react-api-client": "workspace:*",
    "@bene/shared": "workspace:*",
    "@opennextjs/cloudflare": "^1.11.0",
    "@tanstack/react-form": "1.27.5",
    "@tanstack/react-query": "catalog:",
    "better-auth": "catalog:",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.546.0",
    "motion": "^12.24.10",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "5.91.1"
  },
  "nx": {
    "targets": {
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc -p tsconfig.json --noEmit",
          "cwd": "{projectRoot}"
        }
      },
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next dev",
          "cwd": "{projectRoot}"
        },
        "continuous": true,
        "dependsOn": [
          "@bene/gateway:dev"
        ]
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next build",
          "cwd": "{projectRoot}"
        },
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "start": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next start",
          "cwd": "{projectRoot}"
        }
      },
      "cf-build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "opennextjs-cloudflare build",
          "cwd": "{projectRoot}"
        },
        "outputs": [
          "{projectRoot}/.open-next"
        ]
      },
      "cf-deploy": {
        "executor": "nx:run-commands",
        "options": {
          "command": "opennextjs-cloudflare deploy",
          "cwd": "{projectRoot}"
        },
        "dependsOn": [
          "cf-build"
        ]
      },
      "deploy": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx cf-build web",
            "nx cf-deploy web"
          ],
          "parallel": false
        }
      },
      "preview": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx cf-build web",
            "opennextjs-cloudflare preview"
          ],
          "parallel": false
        }
      }
    },
    "tags": [
      "scope:frontend",
      "type:app",
      "platform:web",
      "runtime:worker"
    ]
  }
}
