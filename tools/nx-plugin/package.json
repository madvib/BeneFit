{
  "name": "@bene/nx-plugin",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bene": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "generators": "./generators.json",
  "plugins": "./plugins.json",
  "dependencies": {
    "@nx/devkit": "^22.1.3",
    "toml": "3.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "options": {
          "assets": [
            {
              "input": "./tools/nx/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/nx",
              "glob": "generators.json",
              "output": "."
            }
          ]
        }
      }
    }
  }
}
